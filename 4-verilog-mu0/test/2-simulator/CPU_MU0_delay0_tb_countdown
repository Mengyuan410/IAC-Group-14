    .scope S_0x7fffe9750840;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffe979a3e0_0, 0, 1;
    %pushi/vec4 10000, 0, 32;
T_4.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_4.1, 5;
    %jmp/1 T_4.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 1000, 0;
    %load/vec4 v0x7fffe979a3e0_0;
    %nor/r;
    %store/vec4 v0x7fffe979a3e0_0, 0, 1;
    %delay 1000, 0;
