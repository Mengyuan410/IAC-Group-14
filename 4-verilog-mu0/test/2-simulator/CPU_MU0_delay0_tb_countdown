T_5 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x7fffe979a6d0_0, 0;
    %wait E_0x7fffe97448e0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x7fffe979a6d0_0, 0;
    %wait E_0x7fffe97448e0;
    %pushi/vec4 0, 0, 1;
